extends headpart.pug
block content
    p.blockquote.text-center.mt-5= `${raidName} (${date})`
    .col-md-10.mt-5
        table.table.table-striped.table-responsive-lg.table-bordered.ml-5
            thead
                tr
                    th(scope="col") #
                    th(scope="col") Name
                    th(scope="col") Prio 1
                    th(scope="col") Prio 2
            tbody
                each lock, i in locks
                    tr
                        th(scope="row")= i+1
                        td= lock.name
                        td= lock.prio1
                        td= lock.prio2